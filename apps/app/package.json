{
	"name": "app",
	"version": "1.0.0-dev.0",
	"description": "an electron app",
	"type": "module",
	"engines": {
		"node": ">=22"
	},
	"main": "./out/main/index.js",
	"scripts": {
		"dev": "electron-vite dev",
		"build": "electron-vite build",
		"start": "electron-vite preview",
		"lint": "tsc && eslint .",
		"tdd": "vitest --reporter=verbose",
		"test": "vitest run",
		"test:ci": "vitest run --silent --coverage --reporter=dot",
		"test:e2e": "cross-env E2E=true pnpm run build && playwright test",
		"build:win": "pnpm run build && electron-builder --win --config electron-builder.config.ts",
		"build:mac": "pnpm run build && electron-builder --mac --config electron-builder.config.ts",
		"build:linux": "pnpm run build && electron-builder --linux --config electron-builder.config.ts",
		"gen:electron-schema": "node ./scripts/gen-electron-zod.ts"
	},
	"keywords": [],
	"author": "<author>",
	"license": "MIT",
	"dependencies": {
		"@trpc/server": "11.0.0",
		"chalk": "5.4.1",
		"electron-log": "5.3.2",
		"superjson": "2.2.2",
		"systeminformation": "5.25.11",
		"trpc-electron": "0.1.2",
		"zod": "3.24.2"
	},
	"devDependencies": {
		"@electron/notarize": "3.0.1",
		"@playwright/test": "1.51.1",
		"@solidjs/router": "0.15.3",
		"@solidjs/testing-library": "0.8.10",
		"@tailwindcss/container-queries": "0.1.1",
		"@tanstack/solid-query": "5.69.0",
		"@testing-library/dom": "10.4.0",
		"@testing-library/jest-dom": "6.6.3",
		"@testing-library/user-event": "14.6.1",
		"@trpc/client": "11.0.0",
		"@tsconfig/node20": "20.1.4",
		"@types/eslint-plugin-tailwindcss": "3.17.0",
		"@vitest/coverage-v8": "3.0.9",
		"autoprefixer": "10.4.21",
		"electron": "35.0.3",
		"electron-builder": "25.1.8",
		"electron-vite": "3.0.0",
		"eslint-plugin-jest-dom": "5.5.0",
		"eslint-plugin-playwright": "2.2.0",
		"eslint-plugin-solid": "0.14.5",
		"eslint-plugin-tailwindcss": "3.18.0",
		"eslint-plugin-testing-library": "7.1.1",
		"eslint-plugin-vitest": "0.5.4",
		"jsdom": "26.0.0",
		"memfs": "4.17.0",
		"postcss": "8.5.3",
		"solid-js": "1.9.5",
		"tailwind-variants": "1.0.0",
		"tailwindcss": "3.4.17",
		"ts-to-zod": "3.15.0",
		"vite": "6.2.2",
		"vite-plugin-solid": "2.11.6",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "3.0.9",
		"vitest-canvas-mock": "0.3.3"
	}
}
