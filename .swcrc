{
	"$schema": "https://json.schemastore.org/swcrc",
	"module": { "type": "commonjs" },
	"jsc": {
		"paths": { "~/*": ["./src/*"] },
		"parser": { "syntax": "typescript", "tsx": true },
		"transform": {
			"react": { "runtime": "automatic", "importSource": "@emotion/react" }
		}
	},
	"env": {
		"targets": { "node": "16.15", "chrome": "104" },
		"mode": "entry",
		"coreJs": "3"
	}
}
