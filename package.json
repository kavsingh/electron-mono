{
	"name": "ts-electron",
	"version": "1.0.0",
	"description": "TS Electron setup",
	"main": "./out/main/index.js",
	"scripts": {
		"dev": "electron-vite dev",
		"build": "electron-vite build",
		"postinstall": "electron-builder install-app-deps",
		"start": "electron-vite preview",
		"lint": "eslint .",
		"typecheck": "tsc",
		"codecheck": "pnpm test && pnpm typecheck && pnpm lint",
		"test": "pnpm run-p test:node test:renderer",
		"test:node": "vitest run -c vitest.node.config.ts",
		"test:renderer": "vitest run -c vitest.renderer.config.ts",
		"testwatch": "pnpm run-p testwatch:**",
		"testwatch:node": "vitest -c vitest.node.config.ts",
		"testwatch:renderer": "vitest -c vitest.renderer.config.ts",
		"test:e2e": "cross-env E2E=true pnpm build && playwright test",
		"build:win": "pnpm run build && electron-builder --win --config electron-builder.config.cjs",
		"build:mac": "pnpm run build && electron-builder --mac --config electron-builder.config.cjs",
		"build:linux": "pnpm run build && electron-builder --linux --config electron-builder.config.cjs",
		"gen:schema": "ts-node ./tools/gen-electron-zod.ts"
	},
	"keywords": [],
	"author": "<author>",
	"license": "MIT",
	"packageManager": "^pnpm@7.17.1",
	"dependencies": {
		"@solidjs/router": "0.8.3",
		"@trpc/client": "10.37.1",
		"@trpc/server": "10.37.1",
		"electron-trpc": "0.5.2",
		"eventemitter3": "5.0.1",
		"solid-js": "1.7.11",
		"superjson": "1.13.1",
		"systeminformation": "5.18.15",
		"tailwind-merge": "1.14.0",
		"zod": "3.21.4"
	},
	"devDependencies": {
		"@electron/notarize": "2.1.0",
		"@playwright/test": "1.37.0",
		"@tailwindcss/container-queries": "0.1.1",
		"@testing-library/dom": "9.3.1",
		"@testing-library/jest-dom": "5.17.0",
		"@testing-library/user-event": "14.4.3",
		"@tsconfig/node20": "20.1.1",
		"@tsconfig/strictest": "2.0.1",
		"@types/eslint": "8.44.2",
		"@types/node": "20.4.10",
		"@types/prettier": "2.7.3",
		"@types/testing-library__jest-dom": "5.14.9",
		"@typescript-eslint/eslint-plugin": "6.3.0",
		"@typescript-eslint/parser": "6.3.0",
		"@vitest/coverage-v8": "0.34.1",
		"autoprefixer": "10.4.14",
		"cross-env": "7.0.3",
		"electron": "25.5.0",
		"electron-builder": "24.6.3",
		"electron-vite": "1.0.27",
		"eslint": "8.47.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-import-resolver-typescript": "3.6.0",
		"eslint-plugin-deprecation": "1.5.0",
		"eslint-plugin-filenames": "1.3.2",
		"eslint-plugin-import": "2.28.0",
		"eslint-plugin-jest-dom": "5.0.2",
		"eslint-plugin-playwright": "0.15.3",
		"eslint-plugin-prettier": "5.0.0",
		"eslint-plugin-solid": "0.12.1",
		"eslint-plugin-tailwindcss": "3.13.0",
		"eslint-plugin-testing-library": "6.0.0",
		"jsdom": "22.1.0",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.27",
		"prettier": "3.0.1",
		"require-json5": "1.3.0",
		"solid-testing-library": "0.5.0",
		"tailwindcss": "3.3.3",
		"ts-node": "10.9.1",
		"ts-to-zod": "3.1.3",
		"typescript": "5.1.6",
		"vite": "4.4.9",
		"vite-plugin-solid": "2.7.0",
		"vite-tsconfig-paths": "4.2.0",
		"vitest": "0.34.1"
	},
	"pnpm": {
		"overrides": {
			"tsconfig-paths": "^4.0.0"
		}
	}
}
