{
	"name": "ts-electron",
	"version": "1.0.0",
	"description": "TS Electron setup",
	"main": "./out/main/index.js",
	"scripts": {
		"dev": "electron-vite dev",
		"build": "electron-vite build",
		"postinstall": "electron-builder install-app-deps",
		"start": "electron-vite preview",
		"lint": "eslint .",
		"typecheck": "tsc",
		"codecheck": "pnpm test && pnpm typecheck && pnpm lint",
		"test": "pnpm run-p test:node test:renderer",
		"test:node": "vitest run -c vitest.node.config.ts",
		"test:renderer": "vitest run -c vitest.renderer.config.ts",
		"testwatch": "pnpm run-p testwatch:**",
		"testwatch:node": "vitest -c vitest.node.config.ts",
		"testwatch:renderer": "vitest -c vitest.renderer.config.ts",
		"test:e2e": "cross-env E2E=true pnpm build && playwright test",
		"build:win": "pnpm run build && electron-builder --win --config electron-builder.config.js",
		"build:mac": "pnpm run build && electron-builder --mac --config electron-builder.config.js",
		"build:linux": "pnpm run build && electron-builder --linux --config electron-builder.config.js",
		"gen:schema": "ts-node ./tools/gen-electron-zod.ts"
	},
	"keywords": [],
	"author": "<author>",
	"license": "MIT",
	"packageManager": "^pnpm@7.17.1",
	"dependencies": {
		"@solidjs/router": "0.8.2",
		"@trpc/client": "10.19.1",
		"@trpc/server": "10.19.1",
		"electron-trpc": "0.4.2",
		"eventemitter3": "5.0.0",
		"solid-js": "1.7.3",
		"systeminformation": "5.17.12",
		"tailwind-merge": "1.12.0",
		"zod": "3.21.4"
	},
	"devDependencies": {
		"@electron/notarize": "1.2.3",
		"@playwright/test": "1.32.3",
		"@swc/core": "1.3.49",
		"@tailwindcss/container-queries": "0.1.1",
		"@testing-library/dom": "9.2.0",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/user-event": "14.4.3",
		"@tsconfig/node18": "1.0.1",
		"@tsconfig/strictest": "2.0.0",
		"@types/eslint": "8.37.0",
		"@types/node": "18.15.11",
		"@types/prettier": "2.7.2",
		"@types/testing-library__jest-dom": "5.14.5",
		"@typescript-eslint/eslint-plugin": "5.58.0",
		"@typescript-eslint/parser": "5.58.0",
		"@vitest/coverage-c8": "0.30.1",
		"autoprefixer": "10.4.14",
		"cross-env": "7.0.3",
		"electron": "24.1.0",
		"electron-builder": "23.6.0",
		"electron-vite": "1.0.21",
		"eslint": "8.38.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-import-resolver-typescript": "3.5.5",
		"eslint-plugin-deprecation": "1.4.1",
		"eslint-plugin-filenames": "1.3.2",
		"eslint-plugin-import": "2.27.5",
		"eslint-plugin-jest-dom": "4.0.3",
		"eslint-plugin-playwright": "0.12.0",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-solid": "0.12.0",
		"eslint-plugin-tailwindcss": "3.11.0",
		"eslint-plugin-testing-library": "5.10.2",
		"jsdom": "21.1.1",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.21",
		"prettier": "2.8.7",
		"prettier-plugin-tailwindcss": "0.2.7",
		"require-json5": "1.3.0",
		"solid-testing-library": "0.5.0",
		"tailwindcss": "3.3.1",
		"tailwindcss-logical": "3.0.0",
		"ts-node": "10.9.1",
		"ts-to-zod": "3.0.0",
		"typescript": "5.0.4",
		"vite": "4.2.1",
		"vite-plugin-solid": "2.7.0",
		"vite-tsconfig-paths": "4.2.0",
		"vitest": "0.30.1"
	},
	"pnpm": {
		"overrides": {
			"tsconfig-paths": "^4.0.0"
		}
	}
}
