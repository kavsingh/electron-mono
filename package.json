{
	"name": "electron-mono",
	"version": "0.0.0",
	"description": "electron monorepo setup",
	"author": "<author>",
	"repository": {
		"type": "git",
		"url": "git@github.com:kavsingh/electron-mono"
	},
	"keywords": [],
	"license": "MIT",
	"private": true,
	"type": "module",
	"packageManager": "pnpm@10.18.0+sha512.e804f889f1cecc40d572db084eec3e4881739f8dec69c0ff10d2d1beff9a4e309383ba27b5b750059d7f4c149535b6cd0d2cb1ed3aeb739239a4284a68f40cfa",
	"engines": {
		"node": ">=24"
	},
	"scripts": {
		"dev": "nx dev app",
		"build": "nx build app",
		"build:mac": "nx build:mac app",
		"build:linux": "nx build:linux app",
		"build:win:x64": "nx build:win:x64 app",
		"build:win:arm64": "nx build:win:arm64 app",
		"test": "nx run-many -t test --verbose",
		"test:ci": "nx run-many -t test:ci --verbose",
		"test:e2e": "nx run-many -t test:e2e --verbose",
		"lint": "nx run-many -t lint --verbose",
		"clean:nx": "rm -rf .nx && pnpm -r exec rm -rf .nx",
		"clean:modules": "rm -rf node_modules && pnpm -r exec rm -rf node_modules"
	},
	"devDependencies": {
		"@eslint/js": "9.36.0",
		"@kavsingh/eslint-plugin-filenames": "2.0.0-rc.3",
		"@tsconfig/node24": "24.0.1",
		"@tsconfig/strictest": "2.0.6",
		"@types/eslint": "9.6.1",
		"@types/node": "24.6.2",
		"cross-env": "10.1.0",
		"eslint": "9.36.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-import-resolver-typescript": "4.4.4",
		"eslint-plugin-import-x": "4.16.1",
		"globals": "16.4.0",
		"nx": "21.6.3",
		"prettier": "3.6.2",
		"rimraf": "6.0.1",
		"typescript": "5.9.3",
		"typescript-eslint": "8.45.0"
	}
